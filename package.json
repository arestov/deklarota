{
  "name": "dkt",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:arestov/deklarota.git"
  },
  "files": [
    "/core/dist",
    "/view/dist",
    "/router/dist"
  ],
  "scripts": {
    "build:core": "cd core && npm run build",
    "build:view": "cd view && npm run build",
    "build:router": "cd router && npm run build",
    "build": "run-p build:*",
    "dev:core": "cd core && npm run dev",
    "dev:view": "cd view && npm run dev",
    "dev:router": "cd router && npm run dev",
    "dev": "run-p dev:*",
    "test": "jest",
    "test:watch": "jest --watch-all",
    "prepack": "npm run build"
  },
  "sideEffects": false,
  "peerDependencies": {
    "cash-dom": "*"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@jest/globals": "^26.4.2",
    "@types/jest": "^26.0.14",
    "autoprefixer": "6.0.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "css-select": "^1.3.0-rc0",
    "diff": "3.5.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "jscodeshift": "^0.10.0",
    "microbundle": "^0.12.4",
    "npm-run-all": "^4.1.5",
    "postcss": "5.0.5",
    "xmldom": "0.1.19"
  },
  "dependencies": {
    "babel-plugin-memory-consumers-slicer": "github:arestov/babel-plugin-memory-consumers-slicer#5d6da44",
    "requirejs": "^2.3.6"
  }
}
